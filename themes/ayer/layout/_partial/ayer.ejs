<% if (theme.cover.enable) { %>
<section class="cover">
    <% if (theme.github.enable && theme.github.url){ %>
      <%# "GitHub Ribbons" %>
      <a class="forkMe" href="<%=theme.github.url%>"
        target="_blank"><img width="149" height="149" src="<%- url_for('images/forkme.png') %>"
          class="attachment-full size-full" alt="Fork me on GitHub" data-recalc-dims="1"></a>
    <% } %>
  <div class="cover-frame">
    <div class="bg-box">
        <img src="<%- theme.cover.path %>" alt="image frame" style="filter: brightness(85%);"/>
    </div>
    <div class="cover-inner text-center text-white">
      <h1><a href="<%- url_for() %>"><%= config.title %></a></h1>
      <div id="subtitle-box">
        <% if (theme.subtitle.enable) { %>
        <span id="subtitle"></span>
        <% }else{ %>
          <span id="subtitle"><%= theme.subtitle.text %></span>
        <% } %>
      </div>
      <div>
        <% if (theme.cover.logo) { %>
        <img
          src="<%- theme.cover.logo %>"
          class="cover-logo"
          alt="<%= config.title %>"
        />
        <% } %>
      </div>
      <div style="border-radius: 8px; display: inline-block; padding: 10px 15px 5px; background-color: #0009;">
          <a href="https://github.com/AItsuki" target="_blank" title="github" style="outline: none; background-color: transparent; text-decoration: none; margin: 5px;">
              <img src="https://cdn.jsdelivr.net/gh/moezx/cdn@3.1.9/img/Sakura/images/sns/github.png" alt="github" style="height: 24px; width: 24px;">
          </a>
          <a href="https://aitsuki.blog.csdn.net/" target="_blank" title="csdn" style="outline: none; background-color: transparent; text-decoration: none; margin: 5px;">
              <img src="https://cdn.jsdelivr.net/gh/moezx/cdn@3.1.9/img/Sakura/images/sns/csdn.png" alt="csdn" style="height: 24px; width: 24px;">
          </a>
          <a href="https://www.jianshu.com/u/10782b640adf" target="_blank" title="jianshu" style="outline: none; background-color: transparent; text-decoration: none; margin: 5px;">
              <img src="https://cdn.jsdelivr.net/gh/moezx/cdn@3.1.9/img/Sakura/images/sns/jianshu.png" alt="jianshu" style="height: 24px; width: 24px;">
          </a>
          <a href="https://twitter.com/aitsuki17" target="_blank" title="twitter" style="outline: none; background-color: transparent; text-decoration: none; margin: 5px;">
              <img src="https://cdn.jsdelivr.net/gh/moezx/cdn@3.1.9/img/Sakura/images/sns/twitter.png" alt="twitter" style="height: 24px; width: 24px;">
          </a>
          <a href="mailto: aitsuki@qq.com" title="email" style="outline: none; background-color: transparent; text-decoration: none; margin: 5px;">
            <img src="https://cdn.jsdelivr.net/gh/moezx/cdn@3.1.9/img/Sakura/images/sns/email.svg" alt="email" style="height: 24px; width: 24px;">
        </a>
      </div>
    </div>
  </div>
  <div class="cover-learn-more">
    <a href="javascript:void(0)" class="anchor"><i class="ri-arrow-down-line"></i></a>
  </div>
</section>
<% } %>

<% if (theme.subtitle.enable){ %>
<script src="https://cdn.jsdelivr.net/npm/typed.js@2.0.11/lib/typed.min.js"></script>
<% } %>

<!-- Subtitle -->
<% if (theme.subtitle.enable){ %>
  <script>
    try {
      var typed = new Typed("#subtitle", {
        strings: ['<%= theme.subtitle.text %>', '<%= theme.subtitle.text2 %>', '<%= theme.subtitle.text3 %>'],
        startDelay: <%= theme.subtitle.startDelay %>,
        typeSpeed: <%= theme.subtitle.typeSpeed %>,
        loop: <%= theme.subtitle.loop %>,
        backSpeed: <%= theme.subtitle.backSpeed %>,
        showCursor: <%= theme.subtitle.showCursor %>
      });
    } catch (err) {
      console.log(err)
    }
  </script>
  <% } %>